    <script type="text/javascript" src="/javascripts/room.js"></script>
  </head>

  <body>
    <div id="screen" style="width:100%;color:#fff5ee;background-color:#555;">
      
    </div>

    <div id="info" style="width:100%;padding:2px 5px 2px 5px;color:#fff0f5;background-color:#444;">
